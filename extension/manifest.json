{
    "manifest_version": 3,
    "name": "Notes Extension",
    "version": "1.0",
    "description": "A browser extension for taking notes",
    "permissions": ["storage", "identity"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "index.html",
      "default_icon": "title.png"
    }
  }
  